{
  "family": "dev-rmw-alb",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "dev-rmw-alb-container",
      "image": "<IMAGE1_NAME>",
      "cpu": 0,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [],
      "mountPoints": [],
      "volumesFrom": [],
      "secrets": [
        {
          "name": "AWS_REGION",
          "valueFrom": "/DEV/AWS_REGION"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/dev-rmw-alb",
          "awslogs-region": "eu-west-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512",
  "taskRoleArn": "arn:aws:iam::376937878789:role/dev-rmw-alb-ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::376937878789:role/dev-rmw-alb-ecsTaskExecutionRole"
}
